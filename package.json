  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run checkall"
    }
  },
  "size-limit": [
    {
      "path": "public/*.js"
    }
  ],
  "scripts": {
    "build": "npm install && npm run reset && gatsby build --prefix-paths",
    "build:package": "tar -C public -zcvf release.tar.gz ./ && rm -rf public/",
    "build:test": "STAGE=test GATSBY_API_KEY=\"\" npm run build",
    "build:prod": "STAGE=prod GATSBY_API_KEY=\"\" npm run build",
    "checkall": "npm run stylelint && npm run tscheck && npm run jslint",
    "clean": "rm -rf node_modules/*",
    "develop": "gatsby develop -p 3000 -H 0.0.0.0",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "jsfix": "eslint --fix \"src/**/*.{js,jsx}\"",
    "jslint": "env FORCE_COLOR=1 eslint \"src/**/*.{js,jsx}\"",
    "tscheck": "env FORCE_COLOR=1 tsc --resolveJsonModule --allowJs",
    "reset": "rm -rf .cache/ public/ types/",
    "stylelint": "stylelint '**/*.scss'",
    "serve": "gatsby serve",
    "test": "STAGE=test jest -- tradeApiClient",
    "test:ui": "jest --testPathIgnorePatterns node_modules .cache types tradeApiClient",
    "test:coverage": "npm run test:ui -- --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run --record --key=c6eac74f-5095-4c82-979c-5f97c8ebcdd9",
    "cy:test": "start-server-and-test develop http://localhost:3000 cy:open",
    "cy:ci": "start-server-and-test develop http://localhost:3000 cy:run",
    "cy:ci:test": "STAGE=test CYPRESS_baseUrl=http://localhost:9000/app start-server-and-test serve http://localhost:9000/app cy:run",
    "cy:ci:prod": "STAGE=prod CYPRESS_baseUrl=http://localhost:9000/app start-server-and-test serve http://localhost:9000/app cy:run",
    "semantic-release": "semantic-release"
  }
