  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run checkall"
    }
  },
  "size-limit": [
    {
      "path": "public/*.js"
    }
  ],
  "scripts": {
    "build": "npm install && npm run reset && NODE_OPTIONS=--max-old-space-size=8192 gatsby build --prefix-paths",
    "build:package": "tar -C public -zcvf release.tar.gz ./",
    "build:test": "STAGE=test GATSBY_NEWRELIC_ENV=test GATSBY_API_KEY=\"\" npm run build",
    "build:staging": "STAGE=staging GATSBY_NEWRELIC_ENV=staging GATSBY_API_KEY=\"\" npm run build",
    "build:prod": "STAGE=prod GATSBY_NEWRELIC_ENV=prod GATSBY_API_KEY=\"\" npm run build",
    "checkall": "npm run stylelint && npm run tscheck && npm run jslint",
    "clean": "rm -rf node_modules/*",
    "develop": "gatsby develop -p 3001 -H 0.0.0.0",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "jsfix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "jslint": "env FORCE_COLOR=1 eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "tscheck": "env FORCE_COLOR=1 tsc --resolveJsonModule --allowJs",
    "reset": "rm -rf .cache/ public/ types/",
    "stylelint": "stylelint '**/*.{scss,tsx}'",
    "serve": "gatsby serve --prefix-paths",
    "test": "jest",
    "test:ui": "jest --testPathIgnorePatterns node_modules .cache types tradeApiClient",
    "test:coverage": "npm run test:ui -- --coverage",
    "cy:open": "cypress open",
    "cy:open-dev": "CYPRESS_baseUrl=http://localhost:3000 npm run cy:open",
    "cy:open-build": "CYPRESS_baseUrl=http://localhost:9000/app start-server-and-test serve http://localhost:9000/app cy:open",
    "cy:run": "cypress run",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
